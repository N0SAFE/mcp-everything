#!/usr/bin/env node

import { Command } from "commander";

const program = new Command();

program
  .name('{{ name }}')
  .description('{{ description }}')
  .version("0.0.0");

program.parse(process.argv);

const selfStart = (callback: () => void) => {
  return callback()
}

selfStart(function test() {
  console.log('Hello, World!')
})
